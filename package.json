    "@angular/router": "11.2.5",
    "@fortawesome/fontawesome-free": "5.15.3",
    "@popperjs/core": "2.9.1",
    "@tailwindcss/forms": "0.2.1",
    "chart.js": "2.9.4",
    "ngx-toastr": "13.2.1",
    "rxjs": "6.6.6",
